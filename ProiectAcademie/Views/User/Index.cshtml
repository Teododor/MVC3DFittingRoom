@model List<Proiect.BusinessLogic.Implementation.User.Models.UserModel>

@inject Proiect.Common.DTOs.CurrentUserDto CurrentUser;

<h2>DataBase Users</h2>

<table>

@*    <button type="button" type="btn btn-primary" >
        <a href="@Url.Action("ModifyData", "User")">Modify Data</a>

    </button>*@


    <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("ModifyData", "User")'">Modify Data</button>

   
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    @foreach (var user in Model) 
    {
        
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
        </tr>
    }
</table>